<div class="Browser-panel Browser-notes vf-content" data-role="tab-panel" id="browser-{{ entity.id }}-panel-notes">
    <div class="pattern-library-notes pattern-library-notes--condensed">
        {% if entity.notes %}
        {{ frctl.docs.renderString(entity.notes, renderEnv) | async }}
        {% else %}
        <p class="Browser-isEmptyNote">There are no notes for this item.</p>
        {% endif %}
    </div>
</div>
